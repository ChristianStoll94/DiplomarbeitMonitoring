{
  "ApplicationSettings": {
  },
  "Serilog": {
    "IsEnabled": true,
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Error",
        "Elastic": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs\\Eps.Service.Demo_.log",
          "retainedFileCountLimit": 5,
          "rollingInterval": "Day",
          "shared": true,
          "flushToDiskInterval": "0:00:02",
          "outputTemplate": "{Timestamp:dd.MM.yyyy HH:mm:ss.fff}; {SourceContext}; {Level}; {Message:lj}; {Properties}{NewLine}{Exception}"
        }
      }
    ]
  },
  "Swagger": {
    "IsEnabled": true
  },
  "ElasticApm": {
    "SecretToken": "",
    "ServerUrls": "http://localhost:8200", //Set custom APM Server URL (default: http://localhost:8200)
    "ServiceName": "MyApp", //allowed characters: a-z, A-Z, 0-9, -, _, and space. Default is the entry assembly of the application
    "Environment": "production" // Set the service environment
  }
}