{
  "ApplicationSettings": {
  },
  "Serilog": {
    "Using": [
      "Destructurama.Attributed"
    ],
    "Destructure": [
      {
        "Name": "UsingAttributes"
      }
    ],
    "IsEnabled": true,
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Error",
        "Elastic": "Debug"
      }
    },
    "Enrich": [ "WithElasticApmCorrelationInfo" ],
    "WriteTo": [
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://elastic:njUEHyhELnXHcwfYJdB9@localhost:9200",
          "indexFormat": "eps.service.demo.monitoring-logs-w10template",
          "templateName": "epsServiceTemplate",
          "restrictedToMinimumLevel": "Information",
          "autoRegisterTemplate": true,
          "autoRegisterTemplateVersion": "ESv7"
        }
      },
      {
        "Name": "File",
        "Args": {
          "restrictedToMinimumLevel": "Information",
          "path": "Logs\\Eps.Service.Demo_.log",
          "retainedFileCountLimit": 5,
          "rollingInterval": "Day",
          "shared": true,
          "flushToDiskInterval": "0:00:02",
          "outputTemplate": "{Timestamp:dd.MM.yyyy HH:mm:ss.fff}; {SourceContext}; {Level}; {Message:lj}; {Properties}{NewLine}{Exception}"
        }
      }

    ]
  },
  "Swagger": {
    "IsEnabled": true
  },
  "ElasticApm": {
    "IsEnabled": true,
    "SecretToken": "",
    "ServerUrls": "http://localhost:8200", //Set custom APM Server URL (default: http://localhost:8200)
    "ServiceName": "Eps.Service.Demo.Monitoring", //allowed characters: a-z, A-Z, 0-9, -, _, and space. Default is the entry assembly of the application
    "Environment": "development", // Set the service environment
    "DisableMetrics": "system.process.cgroup.*",
    "CloudProvider": "none",
    "ApiKey": "LU9MejZYd0J6V3JpeTdyWnFjQXg6RVVyWm8weE9UVmlNR0dkaUEzeDBndw=="
  },
  "OpenTelemetry": {
    "IsEnabled": true,
    "Environment": "development",
    "Sources": [ "Eps.Service.Demo.Monitoring" ],
    "Export": {
      "Service": {
        "IsEnabled": true,
        "ServerUrls": "http://localhost:8200",
        "Headers": "Authorization=ApiKey LU9MejZYd0J6V3JpeTdyWnFjQXg6RVVyWm8weE9UVmlNR0dkaUEzeDBndw=="
      },
      "Console": {
        "IsEnabled": false
      }

    }
  },
  "Metrics": {
    "IsEnabled": true
  },
  "HealthCheck": {
    "Services": [

    ],
    "DiskStorages": [

    ],
    "Memory": {
      "MaxMemoryAllocated": 512
    }
  }
}